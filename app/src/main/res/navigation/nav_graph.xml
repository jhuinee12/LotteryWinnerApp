<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/WinnerFragment">

    <fragment
        android:id="@+id/WinnerFragment"
        android:name="com.zahi.lotto.presentation.winner.WinnerFragment"
        tools:layout="@layout/fragment_winner">

        <action
            android:id="@+id/action_FirstFragment_to_SecondFragment"
            app:destination="@id/RecommendedFragment" />
    </fragment>
    <fragment
        android:id="@+id/RecommendedFragment"
        android:name="com.zahi.lotto.presentation.recommended.RecommendedFragment"
        tools:layout="@layout/fragment_recommended">

        <action
            android:id="@+id/action_SecondFragment_to_FirstFragment"
            app:destination="@id/WinnerFragment" />
    </fragment>
</navigation>